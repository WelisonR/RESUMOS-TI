    "\n",
    "def bom_dia():\n",
    "    print('Bom dia!')\n",
    "\n",
    "\n",
    "def boa_tarde():\n",
    "    print('Boa tarde!')\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    executar(bom_dia)"
   ]
